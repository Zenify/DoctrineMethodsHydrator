{
	"name": "zenify/doctrine-methods-hydrator",
	"description": "Hydrates presenter methods from parameters to entities",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.5",
		"kdyby/doctrine": "~2.0",
		"nette/application": "~2.2"
	},
	"require-dev": {
		"nette/bootstrap": "~2.2",
		"nette/di": "~2.2",
		"nette/http": "~2.2",
		"nette/mail": "~2.2",
		"nette/safe-stream": "~2.2",
		"zenify/coding-standard": "~3.0",
		"jakub-onderka/php-parallel-lint": "~0.8",
		"phpunit/phpunit": "~4.4"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineMethodsHydrator\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineMethodsHydrator\\Tests\\": "tests"
		}
	}
}
